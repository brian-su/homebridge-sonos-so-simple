{
    "displayName": "Sonos So Simple",
    "name": "homebridge-sonos-so-simple",
    "version": "0.0.2",
    "description": "A very simple plugin for controlling Sonos Beam/Arc via Homebridge.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git://https://github.com/brian-su/homebridge-sonos-so-simple.git"
    },
    "bugs": {
        "url": "https://github.com/brian-su/homebridge-sonos-so-simple/issues"
    },
    "engines": {
        "node": ">=10.17.0",
        "homebridge": ">=1.0.0"
    },
    "main": "dist/index.js",
    "scripts": {
        "prettify": "prettier --write 'src/*.{js,ts}' '*.json'",
        "watch": "npm run build && npm link && nodemon",
        "build": "rimraf ./dist && tsc",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "homebridge-plugin"
    ],
    "dependencies": {
        "sonos": "^1.14.0"
    },
    "devDependencies": {
        "@types/node": "^14.14.22",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "homebridge": "^1.2.5",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}
