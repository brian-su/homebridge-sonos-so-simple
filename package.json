{
    "displayName": "Sonos So Simple",
    "name": "homebridge-sonos-so-simple",
    "version": "0.3.0",
    "description": "A very simple plugin for controlling Sonos devices via Homebridge.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git://github.com/brian-su/homebridge-sonos-so-simple.git"
    },
    "bugs": {
        "url": "https://github.com/brian-su/homebridge-sonos-so-simple/issues"
    },
    "engines": {
        "node": ">=12.22.7",
        "homebridge": ">=1.0.0"
    },
    "main": "dist/index.js",
    "scripts": {
        "pretty": "prettier --write 'src/*.ts' 'src/*/*.ts' '*.json' ",
        "watch": "npm run build && npm link && nodemon",
        "build": "rimraf ./dist && tsc",
        "prepublishOnly": "npm run build",
        "localDev": "npm run build && npm link && homebridge -D -I",
        "trackDev": "npm run watch",
        "prepare": "husky install"
    },
    "keywords": [
        "homebridge-plugin"
    ],
    "dependencies": {
        "detect-port": "^1.5.1",
        "express": "^4.18.2",
        "sonos": "^1.14.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "@types/node": "^14.14.22",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "homebridge": "^1.2.5",
        "husky": "^8.0.2",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.9.4"
    },
    "overrides": {
        "minimist": ">=1.2.6"
    }
}
